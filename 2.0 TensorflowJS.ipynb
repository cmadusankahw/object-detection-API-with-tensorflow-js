    "\n",
    "app=Flask(__name__)\n",
    "\n",
    "@app.route('/')\n",
    "\n",
    "def index():\n",
    "    return render_template('index.html')\n",
    "\n",
    "@app.route('/upload')\n",
    "\n",
    "def upload():\n",
    "    return render_template('upload.html')\n",
    "\n",
    "app.run(debug=True)"
   ]
